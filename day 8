#include<iostream>
#include<climits>
using namespace std;
int main()
{
    int array[]={7,13,4,5};
    int size=6;
    int max= INT_MIN;
    
    for (int i = 0; i < size; i++)
    {
        /* code */
        if (array[i]>max)
        {
            /* code */
            max=array[i];
        }
        
       
        

    }
    cout<<max;
    

    

    return 0;
}
