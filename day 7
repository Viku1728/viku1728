#include<iostream>
#include<vector>
using namespace std;




int search( int n, int arr[]){
//  int size=6;
int target=7;
for (int i =0; i<=n  ; i++)
{
    /* code */
    if(arr[i]==target)
    {
    return i;
    }
}
};
int main()
{
    int arr[]={4,5,6,7,8,8};
    int n= 5;
    cout<<search(n,arr);
    return 0;
}

